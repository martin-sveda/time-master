{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "g++",
            "args": [
                "-g",
                "-std=c++11",
                "${workspaceFolder}/src/main.cpp",
                "${workspaceFolder}/src/clock/clock.cpp",
                "${workspaceFolder}/src/timer/timer.cpp",
                "${workspaceFolder}/src/time_sources/gps_time_source.cpp",
                "-I${workspaceFolder}/include",
                "-o",
                "build/program"
            ],
            "group": "build"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "C:/msys64/ucrt64/bin/g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-std=c++11",
                "${workspaceFolder}/src/clock/clock.cpp",
                "${workspaceFolder}/src/timer/timer.cpp",
                "${workspaceFolder}/src/time_sources/gps_time_source.cpp",
                "-I${workspaceFolder}/include",
                "${file}",
                "-o",
                "${fileDirname}\\program.exe"
            ],
            "options": {
                "cwd": "C:/msys64/ucrt64/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}